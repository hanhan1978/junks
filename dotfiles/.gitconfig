[user]
    name = hanhan1978
    email = ryo.tomidokoro@gmail.com
[core]
    editor = vim
	excludesfile = /Users/hanhan/.gitignore_global
[alias]
    st = status
    sh = show
    ba = branch -a
    bd = branch -d
    br = branch
    d1 = diff HEAD~
    d2 = diff HEAD~~
    lg = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
    pom = pull origin master
[commit]
    template = ~/.gitmessage
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
